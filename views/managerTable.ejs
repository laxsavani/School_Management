<%-include('managerHeader')%>

<main id="main" class="main">

    <div class="pagetitle">
      <h1>Student Tables</h1>
    </div><!-- End Page Title -->

    <section class="section">
      <div class="row">
        <div class="col-lg-12">

          <div class="card">
            <div class="card-body">
              <h5 class="card-title">Default Table</h5>
              <%if(messages.success){%>

                <div class="alert alert-success">
                  <%=messages.success[0]%>
                  <%console.log(messages.success[0])%>
                </div>
                <%}%> 

              <!-- Default Table -->
              <table class="table">
                <thead>
                  <tr>
                    <th scope="col">Sr No.</th>
                    <th scope="col">Name</th>
                    <th scope="col">Student Email</th>
                    <th scope="col">Student Mobile No.</th>
                    <th scope="col">Details</th>
                  </tr>
                </thead>
                <tbody>
                    <%var a=1%>
                  <%for(var i of datas){%>
                    <tr>
                        <th scope="row"><%=a++%></th>
                        <td><%=i.name%></td>
                        <td><%=i.semail%></td>
                        <td><%=i.smobile%></td>
                        <td><a href="/manager/showSutdentDetail/<%=i.id%>">Details</a></td>
                      </tr>
                    <%}%>
                </tbody>
              </table>
              <!-- End Default Table Example -->
            </div>
      </div>
      </div>
    </section>

  </main><!-- End #main -->
<%-include('footer')%>